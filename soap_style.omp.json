{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#c386f1",
          "properties": {
            "postfix": " ",
            "display_host": false
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#ff479c",
          "properties": {
            "prefix": " Ôùä  ",
            "home_icon": "~",
            "folder_separator_icon": " \uE0b1 ",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#fffb38",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "local_changes_color": "#ff9248",
            "ahead_and_behind_color": "#f26d50",
            "behind_color": "#f17c37",
            "ahead_color": "#89d1dc",
            "stash_count_icon": "\uF692 "
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "properties": {
            "prefix": " \uF898 ",
            "display_version": true
          }
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#111111",
          "background": "#8ED1F7",
          "properties": {
            "prefix": " \uE626 ",
            "display_version": true
          }
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "#111111",
          "background": "#4063D8",
          "properties": {
            "prefix": " \uE624 ",
            "display_version": true
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#111111",
          "background": "#FFDE57",
          "properties": {
            "prefix": " \uE235 ",
            "display_version": true,
            "display_mode": "files",
            "display_virtual_env": false
          }
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#AE1401",
          "properties": {
            "prefix": " \uE791 ",
            "display_version": true,
            "display_mode": "files"
          }
        },
        {
          "type": "azfunc",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#FEAC19",
          "properties": {
            "prefix": " \uf0e7",
            "display_version": false,
            "display_mode": "files"
          }
        },
        {
          "type": "aws",
          "style": "plain",
          "foreground": "#ffffff",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "properties": {
            "prefix": " \uE7AD ",
            "display_default": false
          }
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#111111",
          "background": "#ffff66",
          "properties": {
            "root_icon": "ÔÇ≠"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#f36943",
          "properties": {
            "battery_icon": "",
            "discharging_icon": "Óà± ",
            "charging_icon": "Óà¥ ",
            "charged_icon": "ÓàØ ",
            "color_background": true,
            "charged_color": "#4caf50",
            "charging_color": "#40c4ff",
            "discharging_color": "#ff5722",
            "postfix": "Ôäï "
          }
        },
        {
          "type": "time",
          "style": "plain",
          "invert_powerline": true,
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "\uE0B4",
          "background": "#2e9599",
          "foreground": "#111111"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "spotify",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#1BD760",
          "properties": {
            "playing_icon": "\uE602 ",
            "paused_icon": "\uF8E3 ",
            "stopped_icon": "\uF04D ",
            "track_separator": " - "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#FFFFFF",
          "properties": {
            "prefix": "üåàüê∂",
            "display_exit_code": false,
            "always_enabled": true,
            "error_color": "#ef5350"
          }
        }
      ]
    }
  ],
  "final_space": false
}
